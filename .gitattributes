*.json   text eol=lf
*.less   text eol=lf
*.md     text eol=lf
*.org    text eol=lf
*.py     text eol=lf
*.bash   text eol=lf
*.txt    text eol=lf
*.xml    text eol=lf
*.gv     text eol=lf
*.svg    text eol=lf
*.awk    text eol=lf
Makefile text eol=lf
